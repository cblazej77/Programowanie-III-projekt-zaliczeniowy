                 </image>
             </ImageView>
            <Label fx:id="errorUsername" layoutX="226.0" layoutY="300.0" prefHeight="18.0" prefWidth="216.0" textFill="RED" />
            <TextField fx:id="aLogin" layoutX="225.0" layoutY="274.0" prefHeight="26.0" prefWidth="204.0" promptText="Username" />
