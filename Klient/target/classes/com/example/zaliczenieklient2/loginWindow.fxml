                  <Image url="@redRefresh.png" />
               </image>
            </ImageView>
            <Button fx:id="Button2" layoutX="29.0" layoutY="394.0" mnemonicParsing="false" onAction="#refreshButton" opacity="0.0" prefHeight="25.0" prefWidth="25.0" AnchorPane.bottomAnchor="5.0" AnchorPane.leftAnchor="5.0" />
            <Button layoutX="601.0" layoutY="414.0" mnemonicParsing="false" onAction="#closeApplication" text="Zamknij" />
         </children></AnchorPane>
  </children>
